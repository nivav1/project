def installTerraform() {
          echo 'working'
          echo 'installing terraform'
          echo 'finished...'
}
pipeline {
    agent any
    
    stages {
        stage ('test'){
            steps {
                 sh   '''
                 echo 'jj'
                 echo 'fifa'
                 echo 'ss'
                '''
            }        
            }
        stage ('terraformsss') {
            steps {
               installTerraform()
            }
    }
}
}

